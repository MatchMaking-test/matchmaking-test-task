{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MatchMaking": {
    "MaxPlayersPerMatch": 10,
    "MinPlayersPerMatch": 2,
    "MatchTimeoutSeconds": 300,
    "QueueCheckIntervalSeconds": 5
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8080"
      }
    }
  },  
  "Kafka": {
    "BootstrapServers": "kafka:9092"
  }
}
